-if user_signed_in?
	%p.text-center=link_to "New Pin", new_pin_path
	.container
		.row
			- @pins.each do |pin|
				= link_to (image_tag pin.image.url(:medium)), pin
				%h5.text-left= link_to pin.title, pin
-else
	.h5.btn.btn-default=link_to "New Pin", new_pin_path 
	.container
		.row
			- @pins.each do |pin|
				= link_to (image_tag pin.image.url(:medium)), pin
				%h5.text-left= link_to pin.title, pin


				
				
				
				