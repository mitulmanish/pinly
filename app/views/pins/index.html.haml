-if user_signed_in?
	%h5.text-muted=link_to "New Pin", new_pin_path
	.container
	- @pins.each do |pin|
		= link_to (image_tag pin.image.url(:medium)), pin
		%h2= link_to pin.title, pin
-else
	.h5.btn.btn-default=link_to "New Pin", new_pin_path 
	.container
	- @pins.each do |pin|
		= link_to (image_tag pin.image.url(:medium)), pin
		%h2= link_to pin.title, pin


				
				
				
				